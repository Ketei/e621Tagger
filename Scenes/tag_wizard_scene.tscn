[sub_resource type="GDScript" id="GDScript_0e41a"]
resource_name = "Kneeling"
script/source = "extends MenuButton


func _ready():
	get_popup().hide_on_checkable_item_selection = false
	get_popup().index_pressed.connect(on_index_pressed)


func on_index_pressed(index: int) -> void:
	get_popup().set_item_checked(
			index,
			not get_popup().is_item_checked(index)
			)


func get_tags() -> Array[String]:
	var return_array: Array[String] = []
	
	if get_popup().is_item_checked(0):
		return_array.append(\"on one knee\")
	if get_popup().is_item_checked(1):
		return_array.append(\"all fours\")
	if get_popup().is_item_checked(2):
		return_array.append(\"ass up\")
	
	return return_array


func get_suggestions() -> Array[String]:
	return []


func reset_selection() -> void:
	get_popup().set_item_checked(0, false)
	get_popup().set_item_checked(1, false)
	get_popup().set_item_checked(2, false)

"

