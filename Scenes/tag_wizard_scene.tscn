[sub_resource type="GDScript" id="GDScript_k1tds"]
script/source = "extends HBoxContainer
@onready var body_colors = $BodyColors


func get_tags() -> Array[String]:
	var tags: Array[String] = []
	if 0 < body_colors.value:
		if body_colors.value == 1:
			tags.append(\"monotone fur\")
		elif body_colors.value == 2:
			tags.append(\"two tone fur\")
			tags.append(\"multicolored fur\")
		elif 2 < body_colors.value:
			tags.append(\"multi tone fur\")
			tags.append(\"multicolored fur\")
	return tags


func get_suggestions() -> Array:
	var suggestions: Array[String] = []
	suggestions.append_array(suggestions) 
	return suggestions


"

