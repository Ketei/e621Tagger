resource_name = "force_mult_scenes"
script/source = "extends CheckButton

@onready var shows_multiple_checkbox: CheckBox = $\"../ShowsMultipleCheckBox\"

@onready var cover_check: CheckBox = $\"../ComicElements/Covers/CoverCheck\"
@onready var back_cover_check: CheckBox = $\"../ComicElements/Covers/BackCoverCheck\"
@onready var first_check: CheckBox = $\"../ComicElements/Pages/FirstCheck\"
@onready var end_check: CheckBox = $\"../ComicElements/Pages/EndCheck\"
@onready var page_num_check: CheckBox = $\"../ComicElements/Extras/PageNumCheck\"

func _ready():
	toggled.connect(is_comic_checked)


func is_comic_checked(is_comic: bool) -> void:
	cover_check.disabled = not is_comic
	back_cover_check.disabled = not is_comic
	first_check.disabled = not is_comic
	end_check.disabled = not is_comic
	page_num_check.disabled = not is_comic

"

